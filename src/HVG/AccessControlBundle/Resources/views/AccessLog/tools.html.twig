{% block tools %}
    {% trans_default_domain 'HVGAccessControlBundle' %}
    <div class="row" id="tabs">
        <div class="col-md-12">
            <div class="btn-toolbar">
                <div class="btn-group"><a href="{{ path('accesscontrol_accesslog_index', {'hash' : community.hash, 'checkpoint' : checkpoint.id, 'accessguard' : accessguard.id}) }}" class="btn btn-primary">{{ icon('list fa-fw') }}&nbsp;{{ 'accesslog.index.link'|trans }}</a></div>
                <div class="btn-group"><a href="{{ path('accesscontrol_accesslog_new', {'hash' : community.hash, 'checkpoint' : checkpoint.id, 'accessguard' : accessguard.id}) }}" class="btn btn-success">{{ icon('plus fa-fw') }}&nbsp;{{ 'accesslog.new.link'|trans }}</a></div>
            </div>
            <p></p>
        </div>
    </div>
{% endblock %}