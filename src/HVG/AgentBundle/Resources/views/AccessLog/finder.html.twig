{% block finder %}
    {% set translation_domain = 'HVGAgentBundle' %}{% trans_default_domain translation_domain %}
    <div class="row" id="finder">
        <div class="col-md-12">
        {% if communities is not empty %}
            <ul class="nav nav-pills">
                {% for current in communities %}
                    <li {% if current == community %}class="active"{% endif %}><a href="{{ path('agent_accesslog_index', {'community' : current.id}) }}">{{ current.name }}</a></li>
                {% endfor %}
            </ul>
            <p></p>
        {% endif %}
        </div>
    </div>
    <!-- /#finder -->
{% endblock %}