{% block tools %}
    {% trans_default_domain 'HVGAgentBundle' %}
    <div class="row" id="tools">
        <div class="col-md-12">
            {% if community %}
            <div class="btn-toolbar">
                <div class="btn-group">
                    <a href="{{ path('agent_communityevent_new', {'community' : community.id}) }}" class="btn btn-success">{{ icon('plus fa-fw') }}&nbsp;{{ 'communityevent.new.link'|trans }}</a>
                </div>
            </div>
            <p></p>
            {% endif %}
        </div>
    </div>
    <!-- /#tools -->
{% endblock %}