{% block communities %}
    <div class="row" id="unit-nav">
        <div class="col-md-12">
        {% if communities is not empty %}
            <p>
                <ul class="nav nav-pills">
                    {% for current in communities %}
                        <li {% if current == community %}class="active"{% endif %}><a href="{{ path('agent_datacenter_community', {'id' : current.id }) }}">{{ current.name }}</a></li>
                    {% endfor %}
                </ul>
            </p>
        {% endif %}
        </div>
    </div>
    <!-- /#communities -->
{% endblock %}