{% block tools %}
    {% trans_default_domain 'HVGServiceControlBundle' %}
    <div class="row" id="tabs">
        <div class="col-md-12">
            <div class="btn-toolbar">
                <div class="btn-group"><a href="{{ path('servicecontrol_servicelog_index', {'hash' : community.hash, 'service' : service.id, 'serviceagent' : serviceagent.id}) }}" class="btn btn-primary">{{ icon('list fa-fw') }}&nbsp;{{ 'servicelog.index.link'|trans }}</a></div>
                <div class="btn-group"><a href="{{ path('servicecontrol_servicelog_new', {'hash' : community.hash, 'service' : service.id, 'serviceagent' : serviceagent.id}) }}" class="btn btn-success">{{ icon('plus fa-fw') }}&nbsp;{{ 'servicelog.new.link'|trans }}</a></div>
            </div>
            <p></p>
        </div>
    </div>
{% endblock %}