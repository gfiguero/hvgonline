{% block iconos %}
    {% if app.request.attributes.get( '_route' ) starts with 'main' %}{{ icon('home fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'member' %}{{ icon('users fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'resource' %}{{ icon('truck fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'history' %}{{ icon('book fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'academy' %}{{ icon('graduation-cap fa-fw') }}
    {% endif %}
{% endblock %}
